exports_files(glob([
    '*.xls',
    '*.xlsx',
    '*.xlsm',
    '*.XLS',
    '*.XLSX',
    '*.XLSM',
]))

filegroup(
    name = 'all_files',
    srcs = glob([
        '*.xls',
        '*.xlsx',
        '*.xlsm',
        '*.XLS',
        '*.XLSX',
        '*.XLSM',
    ]),
    visibility = ['//visibility:public'],
)

